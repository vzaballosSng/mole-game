{
  "name": "mole-game",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "build": "webpack",
    "predeploy": "npm run build",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint '**/*.js'",
    "lint:lit-analyzer": "lit-analyzer my-element.js",
    "lit:fix": "eslint . --fix",
    "prepare": "husky install",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.eslintignore --write",
    "start": "webpack serve",
    "prod": "webpack --config ./webpack.config.js",
    "test": "jest",
    "test:coverage": "jest --no-cache --coverage",
    "lint-staged": "echo 'lint-staged is running!' && lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "npx prettier --write '**/*.js'",
      "eslint --fix"
    ]
  },
  "keywords": [
    "web-components",
    "lit-element",
    "javascript",
    "lit"
  ],
  "author": "Victor Zaballos",
  "dependencies": {
    "@babel/core": "^7.24.1",
    "@lit-labs/router": "^0.1.3",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "html-webpack-plugin": "^5.6.0",
    "lit": "^3.0.0",
    "lit-element": "^4.0.4",
    "rxjs": "^7.8.1",
    "urlpattern-polyfill": "^10.0.0",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.3"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-transform-modules-commonjs": "^7.23.3",
    "@babel/preset-env": "^7.24.1",
    "@custom-elements-manifest/analyzer": "^0.6.3",
    "@jest/globals": "^29.7.0",
    "@open-wc/testing": "^3.2.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@testing-library/dom": "^9.3.4",
    "@web/dev-server": "^0.1.31",
    "@web/dev-server-legacy": "^1.0.0",
    "@web/test-runner": "^0.15.0",
    "@web/test-runner-playwright": "^0.9.0",
    "@webcomponents/webcomponentsjs": "^2.8.0",
    "connect-history-api-fallback": "^2.0.0",
    "css-loader": "^6.10.0",
    "eslint": "^8.57.0",
    "gh-pages": "^6.1.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-axe": "^7.0.0",
    "jest-css-modules-transform": "^4.4.2",
    "jest-environment-jsdom-global": "^4.0.0",
    "lint-staged": "^15.2.2",
    "lit-analyzer": "^1.2.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.73.0",
    "rollup-plugin-summary": "^1.4.3",
    "rollup-plugin-terser": "^7.0.2",
    "shadow-dom-testing-library": "^1.11.2",
    "style-loader": "^3.3.4"
  }
